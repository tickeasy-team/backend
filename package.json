    "dotenv": "^16.3.1",
    "ecpay_aio_nodejs": "^1.2.2",
    "ejs": "^3.1.10",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "google-auth-library": "^9.2.0",
    "helmet": "^7.1.0",
    "http-errors": "^2.0.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
