{
  "name": "tickeasy-team-backend",
  "version": "1.0.0",
  "type": "module",
  "description": "tickeasy-backend",
  "main": "dist/bin/server.js",
  "scripts": {
    "start": "node dist/bin/server.js",
    "dev": "tsx watch bin/server.ts",
    "build": "tsc && cp -r views dist/views",
    "build-windows": "tsc && xcopy views dist\\views /E /I /Y",
    "typeorm": "typeorm-ts-node-commonjs",
    "migrate": "npm run typeorm migration:run -- -d config/database.ts",
    "test": "jest",
    "test:faq-search": "npx tsx test-faq-search.js",
    "test:post-refactor": "npx tsx test-post-refactor.js",
    "test:simple-refactor": "npx tsx test-simple-refactor.js",
    "test:service-layer": "npx tsx test-service-layer.js",
    "test:ultra-simple": "node test-ultra-simple.js",
    "test:simple": "node simple-test.js",
    "test:support": "node test-support-api.js",
    "test:support-simple": "node test-simple-support.js",
    "test:semantic-search": "node test-semantic-search.js",
    "test:smart-customer-service": "node test-smart-customer-service.js",
    "init:knowledge-base": "tsx init-knowledge-base.ts",
    "init:simple-kb": "tsx init-simple-kb.ts",
    "init:safe-kb": "tsx init-safe-kb.ts",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix"
  },
  "keywords": [
    "ticketing",
    "concert",
    "express",
    "postgresql",
    "typeorm"
